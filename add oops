#include<iostream>
int main()
{
	int a,b,c,d,e,f,g;
	a=5;
	b=3;
	c=a+b;
	printf("\n");
	std::cout<<"c="<<c;
	a=5;
	b=3;
	d=a-b;
	printf("\n");
	std::cout<<"d="<<d;
    a=5;
	b=3;
	e=a*b;
	printf("\n");
	std::cout<<"e="<<e;
    a=5;
	b=3;
	f=a/b;
	printf("\n");
	std::cout<<"f="<<f;
    a=5;
	b=3;
	g=a%b;
	printf("\n");
	std::cout<<"g="<<g;
	return 0;
  }
